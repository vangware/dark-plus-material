{
  "cSpell.words": [
    "Promisified",
    "vangware"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
